<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="DT.Samples.Agora.Cross.Views.ConnectPage" Title="{Binding Title}" x:Name="ConnectToRoomPage">
    <StackLayout Orientation="Vertical">
        <Entry x:Name="RoomName" Text="{Binding RoomName}"/>
        <Button x:Name="NextButton" Text="Join OneToOne" Clicked="Handle_OneToOne_Clicked"/>
        <Button x:Name="NextMultiButton" Text="Join Group" Clicked="Handle_Group_Clicked"/>
    </StackLayout>
</ContentPage>